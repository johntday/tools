/L20"PowerTerm Scripts" Line Comment = # Escape Char = \ String Chars = "' File Extensions = PSL
/Delimiters = ~!@%^$&*()+=|\/{}[]:;"'<> ,	?
/Function String = "proc ^([a-z,_,0-9]+^) "
/Indent Strings = "{"
/Unindent Strings = "}"
/C1"Switch/Permission"
-all -ascii -code -command -decreasing -errorcode -exact -glob -increasing
-indices -integer -nocase -nocomplain -nonewline -real -regexp
APPEND
CREAT
EXCL
NOCTTY NONBLOCK
PSL_BREAK PSL_CONTINUE PSL_RETURN
RDONLY RDWR
TRUNC
WRONLY
/C2"Reserved words"
break
continue
default
for
if
return
switch
while
